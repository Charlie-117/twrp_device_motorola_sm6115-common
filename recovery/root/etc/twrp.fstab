# mount point	fstype	device					device2				flags
/boot		emmc	/dev/block/bootdevice/by-name/boot					flags=slotselect;flashimg=1
/dtbo       emmc          /dev/block/bootdevice/by-name/dtbo           flags=backup=1;slotselect;flashimg=1
system		/system			ext4		slotselect;logical
vendor		/vendor			ext4		slotselect;logical
product		/product		ext4			slotselect;logical
/super      emmc    /dev/block/bootdevice/by-name/super        flags=backup=1;flashimg=1
/metadata	ext4	/dev/block/bootdevice/by-name/metadata
/data		f2fs	/dev/block/bootdevice/by-name/userdata					flags=fileencryption=ice:aes-256-cts
/recovery   emmc    /dev/block/bootdevice/by-name/recovery                    flags=slotselect;backup=1;flashimg=1
/firmware	vfat	/dev/block/bootdevice/by-name/modem					flags=slotselect;mounttodecrypt;fsflags=ro
/misc		emmc	/dev/block/bootdevice/by-name/misc
/persist    ext4    /dev/block/bootdevice/by-name/persist        flags=backup=1
/bluetooth	emmc	/dev/block/bootdevice/by-name/bluetooth					flags=slotselect;backup=1
/dsp		emmc	/dev/block/bootdevice/by-name/dsp					flags=slotselect;backup=1
/efs1		emmc	/dev/block/bootdevice/by-name/modemst1					flags=backup=1
/efs2		emmc	/dev/block/bootdevice/by-name/modemst2					flags=backup=1;subpartitionof=/efs1
/efsc		emmc	/dev/block/bootdevice/by-name/fsc					flags=backup=1;slotselect
/efsg		emmc	/dev/block/bootdevice/by-name/fsg					flags=backup=1;slotselect

# Removable storage
/external_sd    vfat    /dev/block/mmcblk1p1                                                    flags=removable;storage;display="SDCARD"
/usbstorage      vfat    /dev/block/sdg1                            /dev/block/sdg     flags=fsflags=utf8;display="USB Storage";storage;wipeingui;removable
